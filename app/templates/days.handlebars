{{#if loading}}
    loading days ...
{{else}}
    <ul class="unstyled" >
        {{#each days}}
            <li>
                <a href="#" {{action "loadDate" target="IRC" }} >
                    <strong>{{format date "YYYY-MM-DD"}}</strong>
                </a> ({{count}})
            </li>
        {{/each}}
    </ul>
{{/if}}